{
  "name": "sass-yiq",
  "title": "Sass YIQ",
  "version": "2.0.0",
  "description": "Create more natural color contrasts with Sass.",
  "homepage": "https://github.com/timhettler/sass-yiq",
  "main": "_index.scss",
  "files": [
    "sass/**/*",
    "_index.scss",
    "CHANGELOG.md",
    "LICENSE.txt",
    "README.md"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/timhettler/sass-yiq"
  },
  "bugs": {
    "url": "https://github.com/timhettler/sass-yiq/issues"
  },
  "author": "Tim Hettler <me+github@timhettler.com>",
  "keywords": [
    "sass",
    "color",
    "contrast"
  ],
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "test": "mocha",
    "generate:docs": "sassdoc sass",
    "generate:demo": "sass sass demo/styles",
    "generate": "run-p generate:*",
    "postinstall": "is-ci && husky install",
    "prepare:package": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "json": "^11.0.0",
    "mocha": "^10.2.0",
    "pinst": "^3.0.0",
    "sass": "^1.57.1",
    "sass-true": "^7.0.0",
    "sassdoc": "^2.7.4"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "sass": ">=1.45.0"
  }
}
